<h1>Listing photos</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Conetnt</th>
    <th></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @photos.each do |photo| %>
  <tr>
    <td><%= photo.name %></td>
    <td><%= image_tag(url_for(:action => 'get_image', :id => photo.id), :size => '100x100') %></td>
    <td><%= link_to 'Show', photo %></td>
    <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
    <td><%= link_to 'Destroy', photo, :confirm => 'Are you sure?', :method => :delete %></td>
    <td><%= link_to 'Puzzuule', :action => "puzzuule", :id => photo.id %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Photo', new_photo_path, :class => 'btn primary' %>
